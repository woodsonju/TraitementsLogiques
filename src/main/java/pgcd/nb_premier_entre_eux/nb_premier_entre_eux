//a et b sont premiers entre eux si pgcd(a, b) = 1

def nb_premier_entre_eux(n, nbtirages):
    i = 1
    nbprmiers = 0
    while i <= nbtirages:
        i = i + 1
        a = random.randint(1, n)
        a = random.randint(1, n)
        if pgcd(a, b) == 1:
            nbpremiers = nbpremiers + 1
    return nbpremiers